{
  "name": "gcloud-logger",
  "version": "0.1.18",
  "description": "simple logger for gcp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-pavlenko/gcloud-logger.git"
  },
  "keywords": [
    "logger",
    "google cloud platform",
    "stackdriver"
  ],
  "author": "a-pavlenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-pavlenko/gcloud-logger/issues"
  },
  "homepage": "https://github.com/a-pavlenko/gcloud-logger#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "audit-ci": "^3.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.2",
    "typescript": "^3.9.6"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@google-cloud/logging": "^8.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "js"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "errorOnDeprecated": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -1
      }
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ]
}

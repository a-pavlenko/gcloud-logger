{
  "name": "gcloud-logger",
  "version": "0.1.12",
  "description": "simple logger for gcp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-pavlenko/gcloud-logger.git"
  },
  "keywords": [
    "logger",
    "google cloud platform",
    "stackdriver"
  ],
  "author": "a-pavlenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-pavlenko/gcloud-logger/issues"
  },
  "homepage": "https://github.com/a-pavlenko/gcloud-logger#readme",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/triple-beam": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "audit-ci": "^2.3.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@google-cloud/logging": "^5.2.2",
    "logform": "^2.1.2",
    "triple-beam": "^1.3.0",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "js"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "errorOnDeprecated": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -1
      }
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/"
  ]
}

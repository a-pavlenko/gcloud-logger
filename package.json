{
  "name": "gcloud-logger",
  "version": "0.0.4",
  "description": "simple logger for gcp",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-pavlenko/gcloud-logger.git"
  },
  "keywords": [
    "logger",
    "google cloud platform",
    "stackdriver"
  ],
  "author": "a-pavlenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-pavlenko/gcloud-logger/issues"
  },
  "homepage": "https://github.com/a-pavlenko/gcloud-logger#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^1.0.0-rc.1",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "prettier-tslint": "^0.4.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier-tslint fix",
      "tslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@google-cloud/logging": "^1.2.0",
    "winston": "^3.0.0-rc6"
  }
}
